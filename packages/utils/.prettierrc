{
  "singleQuote": true,
  "jsxSingleQuote": false,
  "trailingComma": "all",
  "printWidth": 100,
  "proseWrap": "never",
  "semi": false,
  "importOrder": [
    "^(react|react-dom|react-router|router-router-dom)$",
    "^(vue|vue-router)$",
    "^([a-z]|@[a-z])",
    "",
    ".*"
  ],
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "overrides": [
    {
      "files": ".prettierrc",
      "options": {
        "parser": "json"
      }
    }
  ]
}
